{{each cinemas}}
<div class="item mb-line-b">
    <div class="title-box">
        <div class="title">
            <span class="item-nm">{{$value.nm}}</span>
            <span class="price-block">
                <span class="price">{{$value.sellPrice}}</span>
                <span class="q">元起</span>
            </span>
        </div>
        <div class="location">
            <div class="detail-location">{{$value.addr}}</div>
            <div class="distance">{{$value.distance}}</div>
        </div>
        <div class="label-block">
            <div class="endorse">改签</div>
            <div class="snack">小吃</div>
            <div class="vipTag">折扣卡</div>
        </div>
        <div class="discount-block">
            <div>
                <div class="card">
                    <img src="data:image/png;base64,"/>
                </div>
                <div class="discount-label-text">{{$value.promotion.cardPromotionTag}}</div>
            </div>
        </div>
        <div class="min-show-block">
            <span>近期场次：</span>
            <span class="time-line">{{$value.showTimes}}</span>
        </div>
    </div>
</div>
{{/each}}